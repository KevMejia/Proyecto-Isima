@model IList<ProyectoIsima.Models.Alumnos>
@{
    ViewBag.Title = "Proyecto Isima";
    Layout = "~/Views/_Layout.cshtml";
}

<h2 class="titulo">Lista de Alumnos</h2>

<table class="table">
    <tr>
    <td>
        <strong>Nombre del Alumno</strong>
    </td>
    <td>
        <strong>Apellido Paterno</strong>
    </td>
    <td>
        <strong>Apellido Materno</strong>
    </td>
    </tr>

    
@for (int i=0; i<Model.Count; i++)
{
    <tr>
        <td>@Html.DisplayFor(model => model[i].NombreAlumno)</td>
        <td>@Html.DisplayFor(model => model[i].ApellidpPatAlumno)</td>
        <td>@Html.DisplayFor(model => model[i].ApellidoMatAlumno)</td>
    </tr>
}
</table>